    "keywords": ["log", "logging", "psr-3"],
    "type": "library",
    "authors": [
        {
            "name": "Johnathan Louie",
            "email": "johnathanlouie88@gmail.com",
            "homepage": "https://jlouie.io",
            "role": "Developer"
        }
