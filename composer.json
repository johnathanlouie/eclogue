{
    "name": "lwd/eclogue",
    "description": "Sends your logs to files, sockets, inboxes, databases and various web services",
    "keywords": ["log", "logging", "psr-3"],
    "type": "library",
    "authors": [
        {
            "name": "Johnathan Louie",
            "email": "johnathanlouie88@gmail.com",
            "homepage": "https://jlouie.io",
            "role": "Developer"
        }
    ],
    "readme": "README.md",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "psr/log": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.36",
        "fzaninotto/faker": "1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Lwd\\Eclogue\\": "src/Lwd/Eclogue/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unit\\Lwd\\Eclogue\\": "tests/Unit/Lwd/Eclogue/",
            "Integration\\Lwd\\Eclogue\\": "tests/Integration/Lwd/Eclogue/"
        }
    },
    "provide": {
        "psr/log-implementation": "^1.1.0"
    }
}
